{
  "expo": {
    "name": "AppSkeleton",
    "slug": "AppSkeleton",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo/profile.png",
    "splash": {
      "image": "./assets/Logo/default-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#08050a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
